{
  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank page"
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "settings": {
    "layout": {
      "contentSize": "1240px",
      "wideSize": "1440px"
    },
    "custom": {
      "typography": {
        "line-height": {
          "normal": 1.35
        }
      }
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "spacingSizes": [
        {
          "name": "1",
          "size": "8px",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "12px",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "16px",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "24px",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "32px",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "clamp(32px, 7.5vw, 40px)",
          "slug": "60"
        },
        {
          "name": "7",
          "size": "clamp(48px, 9vw, 64px)",
          "slug": "70"
        },
        {
          "name": "8",
          "size": "clamp(64px, 12vw, 80px)",
          "slug": "80"
        }
      ]
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "palette": [
        {
          "slug": "primary",
          "color": "#111111",
          "name": "Primary"
        },
        {
          "slug": "primary-second",
          "color": "#2f2f2f",
          "name": "Primary Second"
        },
        {
          "slug": "secondary",
          "color": "#30BAFB",
          "name": "Secondary"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "slug": "background-secondary",
          "color": "#eee",
          "name": "Background Gray"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "fluid": true,
      "lineHeight": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Manrope\", sans-serif",
          "slug": "manrope",
          "name": "Manrope",
          "fontFace": [
            {
              "fontFamily": "Manrope",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Manrope-Regular.woff2"
              ]
            },
            {
              "fontFamily": "Manrope",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Manrope-Medium.woff2"
              ]
            },
            {
              "fontFamily": "Manrope",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/Manrope-SemiBold.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "max": "14px",
            "min": "12px"
          },
          "name": "Tiny",
          "size": "14px",
          "slug": "tiny"
        },
        {
          "fluid": {
            "max": "18px",
            "min": "16px"
          },
          "name": "Small",
          "size": "18px",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "24px",
            "min": "18px"
          },
          "name": "Normal",
          "size": "24px",
          "slug": "normal"
        },
        {
          "fluid": {
            "max": "32px",
            "min": "24px"
          },
          "name": "Medium",
          "size": "32px",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "48px",
            "min": "28px"
          },
          "name": "Large",
          "size": "48px",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "64px",
            "min": "36px"
          },
          "name": "Extra large",
          "size": "64px",
          "slug": "extra-large"
        },
        {
          "fluid": {
            "max": "80px",
            "min": "42px"
          },
          "name": "Huge",
          "size": "80px",
          "slug": "huge"
        },
        {
          "fluid": {
            "max": "81px",
            "min": "64px"
          },
          "name": "Huge on mobile",
          "size": "81px",
          "slug": "huge-mobile"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--primary)"
    },
    "blocks": {
      "core/image": {
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        }
      },
      "core/button": {
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "background": "var(--wp--preset--color--background)",
                "text": "var(--wp--preset--color--primary)"
              }
            },
            "border": {
              "radius": "var(--wp--preset--spacing--10)"
            }
          }
        }
      },
      "core/navigation": {
        "elements": {
          "link": {
            ":hover": {
              "color":  {
                "text": "var(--wp--preset--color--secondary)"
              }
            },
            "border": {
              "style": "none",
              "width": "none"
            },
            "typography": {
              "fontSize": "var(--wp--preset--font-size--small)",
              "lineHeight": "var(--wp--custom--typography--line-height--normal)"
            }
          }
        }
      },
      "core/paragraph": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      },
      "core/list": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/site-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            },
            "border": {
              "bottom": {
                "width": "none",
                "style": "none"
              }
            }
          }
        },
        "spacing": {
          "margin": {
            "bottom": "0px",
            "top": "0px"
          }
        }
      },
      "core/post-featured-image": {
        "elements": {
          "link": {
            "border": {
              "bottom": {
                "width": "none",
                "style": "none"
              }
            }
          }
        },
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        }
      },
      "core/post-title": {
        "elements": {
          "link": {
            "border": {
              "bottom": {
                "width": "none",
                "style": "none"
              }
            },
            "color": {
              "text": "var(--wp--preset--color--primary)"
            }
          }
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--manrope)"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600"
        },
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)",
          "lineHeight": "1.25"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-large)",
          "lineHeight": "1.25"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "1.25"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "lineHeight": "1.25"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "1.25"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.25"
        }
      },
      "link": {
        ":hover": {
          "border": {
            "bottom": {
              "width": "0px",
              "style": "solid"
            }
          }
        },
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "typography": {
          "textDecoration": "none"
        },
        "border": {
          "bottom": {
            "color": "var(--wp--preset--color--secondary)",
            "width": "1px",
            "style": "solid"
          }
        }
      }
    }
  }
}